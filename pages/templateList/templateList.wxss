page {
    background-color: #f3f3f3;
}

.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: flex-start;
}

.tip-frame {
    height: 100rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.icon {
    width: 40rpx;
    height: 40rpx;
}

.tip {
    margin-left: 20rpx;
    font-size: 30rpx;
    color: #bbb;
}

.block {
    width: 670rpx;
    margin-bottom: 40rpx;
    border-radius: 10rpx;
    background-color: #fff;
}

.catalog {
    height: 60rpx;
    padding-left: 40rpx;
    display: flex;
    flex-direction: row;
    align-items: flex-end;
    justify-content: flex-start;
    font-size: 30rpx;
    color: #444;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.section {
    margin-left: 40rpx;
    margin-right: 40rpx;
    height: 100rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.item {
    width: 100%;
    height: 99rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
}

.label {
    width: 540rpx;
    color: #999;
    font-size: 36rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
