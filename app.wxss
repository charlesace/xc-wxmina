/**app.wxss**/
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-user:before { content: "\e89f"; }

.icon-lock:before { content: "\e702"; }

.icon-qrcode:before { content: "\e7dd"; }

.icon-check:before { content: "\e639"; }

.line {
    width: 100%;
    height: 1rpx;
    background-color: #ddd;
}


.panel:first-child {
    margin-top: 0;
}

.panel {
    overflow: hidden;
    margin-top: 20rpx;

    background-color: #fff;
}

.panel__hd {
    padding: 28rpx 30rpx 20rpx;

    color: #999;
    font-size: 26rpx;
    border-bottom: 2rpx solid #ddd;
}


.cell {
    /* position: static;
    border-bottom: 1px solid #ddd;

    font-size: 32rpx; */
    display: flex;

    padding: 10px 15px;
    border-bottom: 1px solid #ddd;

    font-size: 32rpx;

}

.cell__bd {
    flex: 1;
}

.cell__ft {
    padding-right: 13px;
}

.cell__ft_in-access{
    margin-left: 10rpx;

    position:relative;
}

.cell__ft_in-access:after{
    content:" ";
    display:inline-block;
    height:6px;
    width:6px;
    border-width:2px 2px 0 0;
    border-color:#c8c8cd;
    border-style:solid;
    -webkit-transform:matrix(.71,.71,-.71,.71,0,0);
    transform:matrix(.71,.71,-.71,.71,0,0);
}